{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "@popperjs/core@2.11.2",
      "provider": "jsdelivr",
      "files": [
        "dist/umd/popper.js",
        "dist/umd/popper.min.js",
        "dist/umd/popper.min.js.map"
      ],
      "destination": "wwwroot/libs/popper/"
    },
    {
      "library": "jquery@3.6.0",
      "files": [
        "jquery.js",
        "jquery.min.js",
        "jquery.min.map"
      ],
      "destination": "wwwroot/libs/jquery/"
    },
    {
      "library": "bootstrap@5.1.3",
      "provider": "jsdelivr",
      "files": [
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.js.map",
        "dist/js/bootstrap.bundle.min.js.map",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map"
      ],
      "destination": "wwwroot/libs/bootstrap/"
    },
    {
      "library": "@fortawesome/fontawesome-free@6.0.0",
      "provider": "jsdelivr",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "webfonts/**"
      ],
      "destination": "wwwroot/libs/fontawesome"
    },
    {
      "library": "jquery-validate@1.19.3",
      "files": [
        "jquery.validate.min.js"
      ],
      "destination": "wwwroot/libs/jquery-validation/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ],
      "destination": "wwwroot/libs/jquery-validation-unobtrusive/"
    }
  ]
}
